{
  "name": "@sockethub/platform-dummy",
  "description": "A sockethub platform dummy module",
  "version": "3.0.0-alpha.8",
  "type": "module",
  "private": false,
  "author": "Nick Jennings <nick@silverbucket.net>",
  "license": "LGPL-3.0+",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "src/",
    "dist/"
  ],
  "engines": {
    "bun": ">=1.2"
  },
  "keywords": [
    "sockethub",
    "dummy"
  ],
  "devDependencies": {
    "@sockethub/schemas": "^3.0.0-alpha.8",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "@sockethub/server": "^5.0.0-alpha.8"
  },
  "peerDependenciesMeta": {
    "@sockethub/server": {
      "optional": true
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm --sourcemap=external",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sockethub/sockethub.git",
    "directory": "packages/platform-dummy"
  },
  "homepage": "https://github.com/sockethub/sockethub/tree/master/packages/platform-dummy",
  "gitHead": "df82c84a3724c92a9b50e46e9205fa57d834a951"
}
