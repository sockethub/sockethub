{
  "name": "@sockethub/platform-feeds",
  "description": "A sockethub platform module implementing RSS/Atom functionality",
  "version": "4.0.0-alpha.10",
  "type": "module",
  "private": false,
  "author": "Nick Jennings <nick@silverbucket.net>",
  "license": "LGPL-3.0+",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": ["src/", "dist/"],
  "engines": {
    "bun": ">=1.2"
  },
  "keywords": [
    "sockethub",
    "messaging",
    "activitystreams",
    "activity",
    "streams",
    "protocol",
    "feeds",
    "rss",
    "atom"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sockethub/sockethub.git",
    "directory": "packages/platform-feeds"
  },
  "homepage": "https://github.com/sockethub/sockethub/tree/master/packages/platform-feeds",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm --sourcemap=external",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "html-tags": "^3.3.1",
    "htmlparser2": "^9.1.0",
    "podparse": "^1.6.0"
  },
  "devDependencies": {
    "@sockethub/schemas": "^3.0.0-alpha.10",
    "@types/bun": "latest",
    "debug": "^4.4.3"
  },
  "peerDependencies": {
    "@sockethub/server": "5.0.0-alpha.10"
  },
  "peerDependenciesMeta": {
    "@sockethub/server": {
      "optional": true
    }
  },
  "gitHead": "2dc0a34eab5c62afed5a336fc89a8b0318c8b5d4"
}
