{
  "$schema": "@sockethub/schemas/json/sockethub-config.json",
  "examples": true,
  "logFile": "",
  "packageConfig": {
    "@sockethub/activity-streams": {
      "specialObjs": ["credentials"],
      "failOnUnknownObjectProperties": true
    }
  },
  "platforms": [
    "@sockethub/platform-dummy",
    "@sockethub/platform-feeds",
    "@sockethub/platform-irc",
    "@sockethub/platform-metadata",
    "@sockethub/platform-xmpp"
  ],
  "public": {
    "protocol": "http",
    "host": "localhost",
    "port": 10550,
    "path": "/"
  },
  "rateLimiter": {
    "windowMs": 1000,
    "maxRequests": 100,
    "blockDurationMs": 5000
  },
  "redis": {
    "url": "redis://127.0.0.1:6379"
  },
  "sentry": {
    "dsn": "",
    "traceSampleRate": 1.0
  },
  "sockethub": {
    "port": 10550,
    "host": "localhost",
    "path": "/sockethub"
  }
}
