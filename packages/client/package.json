{
  "name": "@sockethub/client",
  "version": "5.0.0-alpha.3",
  "description": "A JavaScript client for the Sockethub protocol gateway",
  "main": "dist/sockethub-client.js",
  "keywords": [
    "sockethub",
    "messaging",
    "activitystreams",
    "activity",
    "streams",
    "protocol",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sockethub/sockethub.git",
    "directory": "packages/client"
  },
  "homepage": "https://github.com/sockethub/sockethub/tree/master/packages/client",
  "scripts": {
    "clean": "npx rimraf dist coverage",
    "clean:deps": "npx rimraf node_modules",
    "compliance": "yarn run lint && yarn run test && yarn run coverage",
    "test": "c8 -x src/bootstrap -x \"src/**/*.test.*\" mocha -r ts-node/register src/*.test.ts src/**/*.test.ts",
    "coverage": "c8 check-coverage --statements 85 --branches 84 --functions 90 --lines 85",
    "lint": "eslint \"**/*.ts\"",
    "lint:fix": "eslint --fix \"**/*.ts\"",
    "build": "tsc && webpack --config webpack.minified.config.js && webpack --config webpack.normal.config.js"
  },
  "author": "Nick Jennings <nick@silverbucket.net>",
  "license": "LGPL-3.0+",
  "devDependencies": {
    "@sockethub/activity-streams": "^4.4.0-alpha.3",
    "@sockethub/schemas": "^3.0.0-alpha.3",
    "@types/chai": "4.3.1",
    "@types/eslint": "8.4.1",
    "@types/eventemitter2": "4.1.0",
    "@types/mocha": "9.1.1",
    "@types/node": "17.0.21",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "10.0.11",
    "@typescript-eslint/parser": "5.20.0",
    "c8": "7.11.2",
    "chai": "4.3.6",
    "eslint": "8.14.0",
    "eslint-cli": "1.1.1",
    "eventemitter2": "6.4.5",
    "mocha": "9.2.2",
    "proxyquire": "2.1.3",
    "sinon": "13.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2"
  }
}
