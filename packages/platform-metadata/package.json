{
  "name": "@sockethub/platform-metadata",
  "version": "1.0.1-alpha.2",
  "description": "Fetch metadata from a given URL",
  "author": "Nick Jennings <nick@silverbucket.net>",
  "license": "LGPL-3.0+",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": ["src/", "dist/"],
  "engines": {
    "bun": ">=1.2"
  },
  "keywords": [
    "sockethub",
    "platform",
    "metadata",
    "open graph",
    "website",
    "preview"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sockethub/sockethub.git",
    "directory": "packages/platform-metadata"
  },
  "homepage": "https://github.com/sockethub/sockethub/tree/master/packages/platform-metadata",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm --sourcemap=external",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "open-graph-scraper": "^6.9.0"
  },
  "devDependencies": {
    "@sockethub/schemas": "^3.0.0-alpha.7"
  },
  "peerDependencies": {
    "@sockethub/server": "5.0.0-alpha.6"
  },
  "gitHead": "b5931fff768dd9be9093df5c01a4fee9fa88068f"
}
